{
  "manifest_version": 3,
  "name": "右クリック禁止を解除",
  "description": "右クリックが禁止されているページで右クリックができるようにする",
  "version": "0.0.1",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*/",
    "http://*/"
  ],
  "icons": {
    "128": "img/128.png",
    "48": "img/48.png",
    "16": "img/16.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "img/icon.png"
  }
}